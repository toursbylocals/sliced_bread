// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Order {
  id         String   @id
  name       String
  quantity   Int
  city       String
  state      String
  country    String
  created_at DateTime @default(now())
}
